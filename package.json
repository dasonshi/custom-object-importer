{
  "name": "api",
  "version": "1.0.0",
  "description": "GoHighLevel Marketplace App for SavvySales.ai",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node server.js",
    "dev": "nodemon server.js",
    "build": "npx prisma generate",
