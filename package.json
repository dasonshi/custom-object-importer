    "@gohighlevel/api-client": "^1.0.1",
    "@prisma/client": "^5.7.0",
    "axios": "^1.11.0",
    "cookie-parser": "^1.4.7",
    "cookie-session": "^2.1.1",
    "cors": "^2.8.5",
    "crypto-js": "^4.2.0",
    "csv-parse": "^6.1.0",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "express-rate-limit": "^8.0.1",
    "multer": "^2.0.2",
